Chart.fetch = function () {
  d3.json(Chart.fetch_url, function (error, json) {
    if (typeof json == 'undefined') {
      alert(error.statusText);
    } else if (typeof json.data == 'undefined') {
      Chart.no_more_data();
    } else {
      Chart.json = json;
      Chart.render(json);
    };
  });
};

Chart.fetch();
