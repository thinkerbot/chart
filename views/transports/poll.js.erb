Chart.fetch = function () {
  d3.json(Chart.transport_url, function (error, json) {
    if (typeof json == 'undefined') {
      alert(error);
    } else if (typeof json.data == 'undefined') {
      Chart.shutdown();
    } else {
      Chart.render(json.data);
      setTimeout(Chart.fetch, 0);
    };
  });
};

Chart.fetch();
